<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="guest">

	<select id="selectGuest" resultType="com.javaex.vo.GuestVo">
		<![CDATA[
			select  no
				    ,name
					,password
					,content
					,reg_date as regDate
			from guestbook
			ORDER BY no desc
		]]>
	</select>

</mapper>